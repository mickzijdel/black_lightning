<% headers = ['View', 'Code', 'Name', 'Is Active'] %>

<% field_sets = @nominal_codes.map { |nominal_code| [nominal_code, nominal_code.code, nominal_code.name, bool_icon(nominal_code.active)] } %>

<%= render 'shared/pages/index', resource_class: Finance::NominalCode, resources: @nominal_codes, headers: headers, field_sets: field_sets %>
