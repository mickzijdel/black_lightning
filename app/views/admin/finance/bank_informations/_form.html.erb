<%= simple_form_for([:admin, @bank_information]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
      <%= f.input :account_holder_name %>
      <%= f.input :sort_code %>
      <%= f.input :account_number %>
  </div>

  <%=f.error :user %>

  <%= render '/shared/form/form_actions', f: f %>
<% end %>
