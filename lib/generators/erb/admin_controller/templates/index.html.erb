<%% headers = ['View'] + <%= attributes.map(&:name).to_s.gsub('"', "'") %> %>

<%% field_sets = @<%= resource_name.pluralize %>.map { |<%= resource_name %>| [<%= resource_name %>,  
<%- attributes.each do |attribute| -%>
  <%= "#{resource_name}.#{attribute.name}," %>
<%- end -%>
] }%>

<%#= attributes.map{|attribute| "#{resource_name}.#{attribute.name}" }.to_s.sub('"', '') %>  

<%%= render 'shared/pages/index', resource_class: <%= class_name %>, resources: @<%= resource_name.pluralize %>, headers: headers, field_sets: field_sets %>
